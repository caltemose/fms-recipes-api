{
  "name": "fms-recipes-api",
  "version": "0.0.1",
  "description": "Recipe management system.",
  "main": "index.js",
  "scripts": {
    "nodemon": "nodemon index.js",
    "webpack": "webpack -w",
    "sass": "node-sass src/sass/main.sass public/styles/main.css -w",
    "start": "npm run sass & NODE_ENV=dev npm run nodemon & npm run webpack",
    "start-live": "npm run sass & NODE_ENV=prod npm run nodemon & npm run webpack",
    "eslint": "eslint src/js",
    "export": "node ./utils/cleanup-export-folder.js && node ./utils/export-recipes.js && node ./utils/export-tag-lists.js",
    "pug": "node ./compile-pug.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caltemose/fms-recipes-api.git"
  },
  "keywords": [
    "recipes",
    "api"
  ],
  "author": "Chad L. Altemose",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caltemose/fms-recipes-api/issues"
  },
  "homepage": "https://github.com/caltemose/fms-recipes-api#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "mongoose": "^4.10.7",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-rc.2",
    "slugg": "^1.2.1",
    "sortablejs": "^1.7.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "camelcase": "^4.1.0",
    "delete": "^1.1.0",
    "eslint": "^4.3.0",
    "load-json-file": "^2.0.0",
    "mongodb": "^2.2.30",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.3"
  }
